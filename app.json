{
  "expo": {
    "name": "Fun Recipes",
    "slug": "fun-recipes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fun-recipes",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/icon.png",
      "splash": {
        "image": "./assets/images/new_logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#F3E5F5"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#F3E5F5",
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "softwareKeyboardLayoutMode": "pan"
      },
      "icon": "./assets/images/icon.png",
      "splash": {
        "image": "./assets/images/new_logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#F3E5F5"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/new_logo.png",
          "imageWidth": 250,
          "resizeMode": "contain",
          "backgroundColor": "#F3E5F5",
          "dark": {
            "backgroundColor": "#4A148C"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "70026d7c-ce2c-4546-8df5-7e52ce00d738"
      }
    },
    "owner": "joaosilva.dev"
  }
}
